    "babel-jest": "^24.1.0",
    "connected-react-router": "^6.3.0",
    "express": "^4.16.4",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1"
  },
