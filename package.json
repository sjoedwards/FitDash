  "scripts": {
    "start": "webpack-dev-server --mode=development --env.PLATFORM=local --env.VERSION=stag --config config/webpack.base.config.js",
    "prebuild": "rm -rf dist && webpack --mode production --config config/webpack.prod.config.js --env.PLATFORM=production --env.VERSION=stag --progress",
    "build": "node server",
    "test": "jest",
    "server": "node server/index.js babel-node",
    "dev": "concurrently \"npm run server\" \"npm run start\"",
    "format": "eslint; tsc *.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/enzyme": "^3.1.17",
